N = int(input())

for _ in range(N):
    X = int(input())
    eh_primo = True
    for i in range(2, int(X**(1/2)) + 1):
        if X % i == 0:         
            eh_primo = False  
            break                         
    if eh_primo == True:       
        print(f"{X} eh primo")  
    if eh_primo == False:      
        print(f"{X} nao eh primo")
